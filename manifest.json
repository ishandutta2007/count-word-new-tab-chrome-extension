{
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "version": "1.0",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_title": "Ishan Counter"
    },
    "name": "count word new tab"
}